{
  "name": "extension-a11y-checker-storage",
  "version": "0.1.0",
  "author": {
    "name": "Lukas Fritze",
    "email": "l.fritze@mittwald.de"
  },
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "compile": "yarn build",
    "format": "prettier $@ '**/*.{ts,tsx,yaml,yml,json,md,mdx}' --write",
    "format:check": "prettier $@ '**/*.{ts,tsx,yaml,yml,json,md,mdx}' --check"
  },
  "dependencies": {
    "@typegoose/typegoose": "^12.11.0",
    "cron-parser": "^4.9.0",
    "mongodb": "^6.12.0",
    "mongoose": "^8.10.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "eslint": "^9",
    "typescript": "^5"
  },
  "packageManager": "yarn@4.6.0"
}
